@if (ViewData["feedback"] != null)
{
    <div class="alert alert-primary" role="alert">
        @ViewData["feedback"]
    </div>}

@if (ViewData["error"] != null)
{
    <div class="alert alert-danger" role="alert">
        @ViewData["error"]
    </div>}